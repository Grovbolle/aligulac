{% extends "index.djhtml" %}

{% comment %}
This template is used for HTTP 404 errors.
{% endcomment %}

{% load i18n %}

{% block title %}404{% endblock %}

{% block content %}
  <div class="row">
    <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1 col-sm-12 col-xs-12 text">
      <h2>404: File not found</h2>
      <p>
        {% blocktrans %}The page you were looking for was nowhere to be found. Sorry!{% endblocktrans %}
      </p>
    </div>
  </div>
{% endblock %}
